{
    "name": "chewcode/laravel-azure-media-services-live-streaming",
    "description": "A laravel 4.1 service provider package that uses guzzle to access the Preview Live Streaming APIs that currently aren't in the native sdk.",
    "keywords"   : ["microsoft", "azure","windows azure","php","laravel"],
    "homepage"   : "https://github.com/chewcode/laravel-azure-media-services-live-streaming",
    "license"    : "MIT",
    "authors": [
        {
            "name"     : "Under New Management",
            "homepage" : "http://github.com/undernewmanagement",
            "role"     : "developer"
        },
        {
            "name"     : "Ben Bowler",
            "homepage" : "http://github.com/benbowler",
            "role"     : "developer"
        }
    ],
    "support" : {
        "issues" : "https://github.com/undernewmanagement/laravel4-azure/issues"
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "guzzle/guzzle": "*"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-0": {
            "Chew\\Laravel\\AzureLive": "src/"
        }
    },
    "minimum-stability": "dev"
}
